{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"module": "es2015",
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "es5",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2018",
			"dom"
		],
		"paths": {
			"@nowzoo/ngx-form": [
				"dist/ngx-form"
			],
			"@nowzoo/ngx-form/*": [
				"dist/ngx-form/*"
			],
			"@nowzoo/ngx-crumbs": [
				"dist/ngx-crumbs"
			],
			"@nowzoo/ngx-crumbs/*": [
				"dist/ngx-crumbs/*"
			],
			"@nowzoo/ngx-message": [
				"dist/ngx-message"
			],
			"@nowzoo/ngx-message/*": [
				"dist/ngx-message/*"
			],
			"@nowzoo/ngx-sign-in-redirect": [
				"dist/ngx-sign-in-redirect"
			],
			"@nowzoo/ngx-sign-in-redirect/*": [
				"dist/ngx-sign-in-redirect/*"
			],
			"@nowzoo/ngx-date-time-inputs": [
				"dist/ngx-date-time-inputs"
			],
			"@nowzoo/ngx-date-time-inputs/*": [
				"dist/ngx-date-time-inputs/*"
			],
			"@nowzoo/ngx-bootstrap-modal": [
				"dist/ngx-bootstrap-modal"
			],
			"@nowzoo/ngx-bootstrap-modal/*": [
				"dist/ngx-bootstrap-modal/*"
			],
			"@nowzoo/ngx-form-helpers": [
				"dist/ngx-form-helpers"
			],
			"@nowzoo/ngx-form-helpers/*": [
				"dist/ngx-form-helpers/*"
			],
			"@nowzoo/ngx-password-toggle": [
				"dist/ngx-password-toggle"
			],
			"@nowzoo/ngx-password-toggle/*": [
				"dist/ngx-password-toggle/*"
			],
			"@nowzoo/ngx-firebase-oob": [
				"dist/ngx-firebase-oob"
			],
			"@nowzoo/ngx-firebase-oob/*": [
				"dist/ngx-firebase-oob/*"
			],
			"@nowzoo/ngx-firebase-auth": [
				"dist/ngx-firebase-auth"
			],
			"@nowzoo/ngx-firebase-auth/*": [
				"dist/ngx-firebase-auth/*"
			],
			"@nowzoo/ngx-fire-sign-in": [
				"dist/ngx-fire-sign-in"
			],
			"@nowzoo/ngx-fire-sign-in/*": [
				"dist/ngx-fire-sign-in/*"
			]
		}
	}
}