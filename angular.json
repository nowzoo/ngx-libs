{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-form": {
      "root": "projects/ngx-form",
      "sourceRoot": "projects/ngx-form/src",
      "projectType": "library",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ngx-form/tsconfig.lib.json",
            "project": "projects/ngx-form/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-form/src/test.ts",
            "tsConfig": "projects/ngx-form/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-form/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-form/tsconfig.lib.json",
              "projects/ngx-form/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ngx-form-demo": {
      "root": "projects/ngx-form-demo/",
      "sourceRoot": "projects/ngx-form-demo/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ngx-form-demo",
            "index": "projects/ngx-form-demo/src/index.html",
            "main": "projects/ngx-form-demo/src/main.ts",
            "polyfills": "projects/ngx-form-demo/src/polyfills.ts",
            "tsConfig": "projects/ngx-form-demo/tsconfig.app.json",
            "assets": [
              "projects/ngx-form-demo/src/favicon.ico",
              "projects/ngx-form-demo/src/assets"
            ],
            "styles": [
              "projects/ngx-form-demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ngx-form-demo/src/environments/environment.ts",
                  "with": "projects/ngx-form-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ngx-form-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ngx-form-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ngx-form-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ngx-form-demo/src/test.ts",
            "polyfills": "projects/ngx-form-demo/src/polyfills.ts",
            "tsConfig": "projects/ngx-form-demo/tsconfig.spec.json",
            "karmaConfig": "projects/ngx-form-demo/karma.conf.js",
            "styles": [
              "projects/ngx-form-demo/src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "projects/ngx-form-demo/src/favicon.ico",
              "projects/ngx-form-demo/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ngx-form-demo/tsconfig.app.json",
              "projects/ngx-form-demo/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
